module arithmetic_expressions;

const 
One = 1;
Two = 2;
Three = 3;
Four = 4;
HundredSixteen = !A0;

var
Result : integer;

begin
	Result := One + Two;
	Result := Result - Three;
	Result := +One;
	Result := -One;
	Result := One * Two;
	Result := Four / Two;
	Result := -12;
	Result := Result + 2;
	Result := Result - 2;
	Result := Result mod 2;
	Result := Result div 2;
	Result := +2 + Result;
	Result := 2 - Result;
	Result := 2 * Result;
	Result := Result + 2 div Result div Result;
	Result := -2 + Result;
	Result := -2 - Result;
	Result := -2 * Result;
	Result := -2 div Result;
	// parantheses describe how the parser works through the expression below this one
	Result := ((Result + ((((Result - 2) * 2) * (8 div Result)))) - 1);
	Result := Result + ((Result - 2) * 2 * 8 div Result) - 1;
	Result := Result + 12 * 8 div ((Result - 2) * 2 * 8 div Result) - 1;
end arithmetic_expressions.
